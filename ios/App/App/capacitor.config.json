{
	"ios": {
		"scheme": "OmniMethodDev"
	},
	"appId": "com.shmorty.omni-method.app",
	"appName": "omni-method",
	"webDir": "www",
	"plugins": {
		"CapacitorHttp": {
			"enabled": true
		},
		"GoogleAuth": {
			"scopes": [
				"profile",
				"email"
			],
			"serverClientId": "164197842062-i2oqluboqhldepb0enpt0hlnmrongsvl.apps.googleusercontent.com",
			"forceCodeForRefreshToken": true
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchAutoHide": true,
			"launchShowDuration": 4000
		}
	},
	"packageClassList": [
		"InAppReviewPlugin",
		"InAppReviewPlugin",
		"FirebaseAnalyticsPlugin",
		"FirebaseAnalyticsPlugin",
		"FirebaseCrashlyticsPlugin",
		"FirebaseCrashlyticsPlugin",
		"AppPlugin",
		"CAPCameraPlugin",
		"HapticsPlugin",
		"LocalNotificationsPlugin",
		"CAPNetworkPlugin",
		"PushNotificationsPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin"
	]
}
